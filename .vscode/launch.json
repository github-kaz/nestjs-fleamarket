{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug NestJS",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "start:debug"],
        "console": "integratedTerminal",
        "restart": true,
        "cwd": "${workspaceFolder}",
        "sourceMaps": true,
        "env": {
          "NODE_ENV": "development"
        },
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
  }